/* Usage code for this ASQL */
select
ael_i(p,'ASQL_log','9806_Instypes') 'Name'
into  ASQL_LOG_TEMP
from TextObject p where 
p.name = 'PCG_Eq_Attrib' and p.type = 'SQL Query' 

select 

i.insid 'Instrument',
i.instype   'Instype'

from
instrument  i,
portfolio   p,
trade t

where
t.insaddr = i.insaddr
and t.prfnbr = p.prfnbr 
and p.prfid = 'Single Stock Options'
and i.exp_day > yesterday

group by 1





