/*Usage for this code*/
select
    ael_i(p,'ASQL_log','SAMM_Advice_Note_PDF') 'Name'
    into ASQL_LOG_TEMP
    from TextObject p where
    p.name = 'SAMM_Advice_Note_PDF' and p.type = 'SQL Query'

/* 
    Name      : SAMM_Advice_Note_PDF
    Purpose   : Build confirmations Advice Notes in PDF 
    Developer : Tshepo Mabena
    Date      : 28-07-2008
*/


select
	ael_s(,'SAMM_Advice_Note_PDF.ASQL')	  /* Opens AEL */
into
	temp
from
	serverdata s
where
	s.srdnbr > 0 