select
ael_i(p,'ASQL_log','SAOps_AddCapitalEventRef') 'Name'
into  ASQL_LOG_TEMP
from TextObject p where 
p.name = 'SAOps_AddCapitalEventRef' and p.type = 'SQL Query' 

