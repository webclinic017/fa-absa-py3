/* Usage code for this ASQL */
select
ael_i(p,'ASQL_log','SAIT_Midbase_Issuers') 'Name'
into  ASQL_LOG_TEMP
from TextObject p where 
p.name = 'SAIT_Midbase_Issuers' and p.type = 'SQL Query' 


SELECT distinct 
    i.issuer_ptynbr,
    p.ptyid,
    ael_i(i,'SAGEN_str_functions.getInstypeENUM',i.insaddr)  'TypeEnum',
    i.insid ,
    i.insaddr
FROM 
    instrument i,
    party p
where
p.ptynbr = i.issuer_ptynbr 