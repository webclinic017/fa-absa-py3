/* Usage code for this ASQL */
select
ael_i(p,'ASQL_log','SAERM_Counterparty_Extract') 'Name'
into  ASQL_LOG_TEMP
from TextObject p where 
p.name = 'SAERM_Counterparty_Extract' and p.type = 'SQL Query' 
select
	p.ptyid				'Name',
	max(time_to_day(t.time))	'LastTraded',
	p.creat_time			'Created',
	p.updat_time			'Updated',
	TODAY				'BusinessDay'
from
	trade t,
	party p
where
	t.counterparty_ptynbr = p.ptynbr
group by p.ptyid