/* ******************  ASQL USAGE LOG  ********************** */ 
 select 
      ael_i(p,'ASQL_log','PCG_Exception_PriceLinkDefn') 'Name' 
 into  ASQL_LOG_TEMP 
 from TextObject p 
 where p.name = 'PCG_Exception_PriceLinkDefn' and p.type = 'SQL Query' 
 
  select
display_id(pd,'insaddr') 'Instrument',
display_id(pd,'source_ptynbr') 'Market',
data[0] 'Translation',
defnbr 'LinkID',
display_id(pd,'disnbr') 'Feed',
display_id(pd,'curr') 'Curr',
keep_intraday_prices
from 
PriceDefinition pd


