/* Usage code for this ASQL */
select
ael_i(p,'ASQL_log','SACM_ILB_Initial_Values') 'Name'
into  ASQL_LOG_TEMP
from TextObject p where 
p.name = 'SACM_ILB_Initial_Values' and p.type = 'SQL Query' 

select 
i.insid,
add_info(i,'ExCoup1')'ExCoup1',
add_info(i,'ExCoup2')'ExCoup2',
l.initial_index_value 

from leg l,
instrument i


where 

i.instype='IndexLinkedBond'
and i.insaddr=l.insaddr



