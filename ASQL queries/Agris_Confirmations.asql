/*Usage for this code*/
select
    ael_i(p,'ASQL_log','Agris_Confirmations') 'Name'
    into ASQL_LOG_TEMP
    from TextObject p where
    p.name = 'Agris_Confirmations' and p.type = 'SQL Query'

