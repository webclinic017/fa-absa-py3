select
    t.trdnbr
from
    Trade t,
    Portfolio p,
    Instrument i
where
    t.prfnbr = p.prfnbr
    and p.prfid in ('MIDAS_FWT', 'Prop_JN', 'MIDAS_FXY', 'MIDAS_AND', 'MIDAS_NZT', 'LTFX_SWAPS', 'MIDAS_HDG', 'MIDAS_CCS', 'JN_FXOptions', 'OTC Platinum', 'MIDAS_JOL', 'MIDAS_ECF', 'MIDAS_NDF', 'Africa_Curr', 'MIDAS_PAV')
    and t.insaddr = i.insaddr
    and i.instype = 'Curr'
    and t.status not in ('Simulated', 'Void', 'Reserved')
    and t.insaddr ~= t.curr
