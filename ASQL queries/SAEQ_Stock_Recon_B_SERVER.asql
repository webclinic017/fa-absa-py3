select
ael_i(p,'ASQL_log','SAEQ_Stock_Recon_B_SERVER') 'Name'
into  ASQL_LOG_TEMP
from TextObject p where 
p.name = 'SAEQ_Stock_Recon_B_SERVER' and p.type = 'SQL Query' 

select
    ael_s(,'Stock_Recon_Backdated_SERVER.StockRecon')
from
    serverdata s
where
    s.srdnbr > 0