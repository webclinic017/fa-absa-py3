/*Usage for this code*/
select
    ael_i(p,'ASQL_log','SACM_Bond_Confirmation_PDF') 'Name'
    into ASQL_LOG_TEMP
    from TextObject p where
    p.name = 'SACM_Bond_Confirmation_PDF' and p.type = 'SQL Query'

/* 
    Name      : SACM_Bond_Confirmation_PDF
    Purpose   : Build confirmations for Bonds in PDF 
    Developer : Tshepo Mabena
    Date      : 26-06-2008
*/


select
	ael_s(,'SACM_Bond_Confirmation_PDF.ASQL')	  /* Opens AEL */
into
	temp
from
	serverdata s
where
	s.srdnbr > 0 