
select
    g.grpid                                 'Group Name',
    g.description                           'Group Description',
    p.profid                                'Profile',
    p.description                           'ProfileDescription'

from
    front.Group g,
    GroupProfileLink gpl,
    UserProfile p

where
    g.grpnbr = gpl.grpnbr
and gpl.profnbr = p.profnbr
/*and g.grpid like 'PCG Mkt%'*/
/*and  g.grpid like 'OPS %'*/
and  g.grpid like 'FO %'