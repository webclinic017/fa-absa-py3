/* Usage code for this ASQL */
select
ael_i(p,'ASQL_log','SAMR_EQFieldsIndex') 'Name'
into  ASQL_LOG_TEMP
from TextObject p where 
p.name = 'SAMR_EQFieldsIndex' and p.type = 'SQL Query' 


select
	i.insaddr,
	ael_s(i,'SAMR_EQFieldsIndex.EQFieldsIndex',@Outfile{})    
	
from
	instrument i
where
	i.insid = 'ZAR'