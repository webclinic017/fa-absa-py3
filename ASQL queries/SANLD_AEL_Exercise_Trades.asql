/* Usage code for this ASQL */
select
ael_i(p,'ASQL_log','SANLD_AEL_Exercise_Trades') 'Name'
into  ASQL_LOG_TEMP
from TextObject p where 
p.name = 'SANLD_AEL_Exercise_Trades' and p.type = 'SQL Query' 


select
    ael_s(, 'SANLD_AEL_Excercised_Trades.exercise','NLD_All_Trades_MaturedOptions') 'Exercise'
    
from
    Instrument i
    
where
    i.insid = 'ZAR'