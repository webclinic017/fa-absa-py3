/* Usage code for this ASQL */
select
ael_i(p,'ASQL_log','SAGEN_SM_Automated_Payments') 'Name'
into  ASQL_LOG_TEMP
from TextObject p where 
p.name = 'SAGEN_SM_Automated_Payments' and p.type = 'SQL Query' 




select
    ael_s(,'SAGEN_SM_Automated_Payments.Payment')
from
    serverdata s
where
    s.srdnbr > 0