select
    t.trdnbr,
    i.insid,
    t.status,
    i.exp_day,
    t.value_day,
    t.acquire_day,
    t.time,
    display_id(t, 'acquirer_ptynbr'),
    i.instype
    
from
    Trade t,
    Instrument i
    
where
    t.insaddr = i.insaddr
and t.trdnbr in (5576584,
5576602,
5576614,
5576636,
5576657,
5576878,
5576883,
5576899,
5576903,
5576924)
