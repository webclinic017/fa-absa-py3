/* ******************  ASQL USAGE LOG  ********************** */ 
 select 
      ael_i(p,'ASQL_log','CAP_MARKET_BO_PriceforBESA') 'Name' 
 into  ASQL_LOG_TEMP 
 from TextObject p 
 where p.name = 'CAP_MARKET_BO_PriceforBESA' and p.type = 'SQL Query' 
 
  select
today 'Day',
i.insid,
mtm_value_ins(i,today,,,,)*100 'MTM Value per 1 nominal'


from 
instrument i

where
i.insid in ('ZAR/BD/R157/080508/8.30','ZAR/BD/R157/080508/7.90')
