/* ******************  ASQL USAGE LOG  ********************** */ 
 select 
      ael_i(p,'ASQL_log','kk_Vega') 'Name' 
 into  ASQL_LOG_TEMP 
 from TextObject p 
 where p.name = 'kk_Vega' and p.type = 'SQL Query' 
 
select
    vega(t)
from
    trade t
where
	match_filter(t,@Filter{;Tradefilter.fltid})