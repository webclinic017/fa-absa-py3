/* Usage code for this ASQL */
select
ael_i(p,'ASQL_log','ZAR-SWAP-Points') 'Name'
into  ASQL_LOG_TEMP
from TextObject p where 
p.name = 'ZAR-SWAP-Points' and p.type = 'SQL Query' 
/*ZAR-SWAP-POINTS*/

