/* ******************  ASQL USAGE LOG  ********************** */ 
 select 
      ael_i(p,'ASQL_log','Candice Exception ValGroup') 'Name' 
 into  ASQL_LOG_TEMP 
 from TextObject p 
 where p.name = 'Candice Exception ValGroup' and p.type = 'SQL Query' 
 
  select distinct
    list, 
    entry,
    description,
    owner_usrnbr,
    version_id,
    seqnbr
from
    choicelist 
where list='ValGroup'    
order by 1,2