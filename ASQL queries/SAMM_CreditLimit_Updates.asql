select
ael_i(p,'ASQL_log','SAMM_CreditLimit_Updates') 'Name'
into  ASQL_LOG_TEMP
from TextObject p where 
p.name = 'SAMM_CreditLimit_Updates' and p.type = 'SQL Query' 


SELECT 
p.ptyid 'Party', 
u.name 'LastUpdateUser', 
c.updat_time 'UpdateTime',
limit[0], 
limit[1],
credit_external[0]
FROM 
CreditLimit c, Party p, User u
WHERE
c.ptynbr = p.ptynbr
AND
u.usrnbr = c.updat_usrnbr
ORDER BY p.ptyid
