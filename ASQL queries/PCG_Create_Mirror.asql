select
ael_i(p,'ASQL_log','PCG_Create_Mirror') 'Name'
into  ASQL_LOG_TEMP
from TextObject p where 
p.name = 'PCG_Create_Mirror' and p.type = 'SQL Query' 
/*
Purpose: [Created to book mirror trades.]
