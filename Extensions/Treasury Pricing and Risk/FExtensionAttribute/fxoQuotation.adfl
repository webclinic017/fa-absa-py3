[Treasury Pricing and Risk]FInstrument:fxoQuotation = nil;
[Treasury Pricing and Risk]FOption:fxoQuotation = select(isFXOption, 
  true->select(quotation, 
    "Points of UndCurr"->currency.Name + "per" + string((underlying).ForeignCurrency), 
    "Pct of Nominal"->"%" + currency.Name, 
    "Points of BaseCurr"->currency.Name + "per" + strikeCurrency.Name, 
    default->quotation), 
  false->nil);
[Treasury Pricing and Risk]FTradeRow:fxoQuotation = instrument:fxoQuotation [quotation = tradeRowQuotation, currency = tradeRowCurrency];