[_exclusion_list_exts]FInstrumentAndTrades:blacklistedInstrumentsInTheoreticalValue = select(blacklistOrWhitelist, 
  "Whitelist"->applyBlacklistFilter(instrumentsInTheoreticalValueForWhitelist, blacklistedInstrumentsFilterIfAny, false, blacklistRefilterClock), 
  "Blacklist"->applyBlacklistFilter(instrumentsInTheoreticalValueForBlacklist, blacklistedInstrumentsFilterIfAny, true, blacklistRefilterClock), 
  default->throwError("Invalid blacklist type"));