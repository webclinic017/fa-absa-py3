[ABSAPortfolioSwap]FDeposit:cfdCallAccountBalance = (-stripDateTimePart(redemptionAmountProjectedCashFlow) - firstLeg :* "cfdCallAccountBalance" [profitAndLossEndDate]) * riskPosition;
[ABSAPortfolioSwap]FInstrument:cfdCallAccountBalance = nil;
[ABSAPortfolioSwap]FInstrumentAndTrades:cfdCallAccountBalance = (sumOf(instruments :* "cfdCallAccountBalance" [positionObject, priceSource, valuesOnSpotOverride]));
[ABSAPortfolioSwap]FLeg:cfdCallAccountBalance = sumOf(stripDateTimePart(cfdfutureAdjustableCashFlows :* "projectedCashFlow" [legInformation]));
[ABSAPortfolioSwap]FMultiInstrumentAndTrades:cfdCallAccountBalance = (sumOf(instruments :* "cfdCallAccountBalance" [positionObject, priceSource, valuesOnSpotOverride]));