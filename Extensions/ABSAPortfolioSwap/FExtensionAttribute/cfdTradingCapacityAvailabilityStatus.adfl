[ABSAPortfolioSwap]FInstrumentAndTrades:cfdTradingCapacityAvailabilityStatus = switch(cfdTradingCapacityAvailability, 
  nil->nil, 
  default->switch(cfdTradingCapacityAvailability > 10, 
    true->"Within", 
    false->switch(cfdTradingCapacityAvailability > 0, 
      true->"Warning", 
      default->"Breached")));