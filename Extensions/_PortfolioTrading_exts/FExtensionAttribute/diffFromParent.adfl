[_PortfolioTrading_exts]FInstrumentAndTrades:diffFromParent = switch(changePercentInverseVariable, 
  nil->0, 
  default->float((parent:changePercent [targetColumnSetExternally, distributionTypeSetExternally] / 100) * (object :* getExtensionAttribute) - (changePercentInverseVariable / 100) * (object :* getExtensionAttribute)));