[_brokerage_risk_exts]FInstrument:equity = equityPremium + collateralValueAdjusted;
[_brokerage_risk_exts]FMultiInstrumentAndTrades:equity = convertToCurrency(sumOf(instruments :* "collapse(equity)" [priceSource, valuesOnSpotOverride, positionObject]), portfolioFxRates, displayCurrencySymbol, filterUnit);
[_brokerage_risk_exts]FSingleInstrumentAndTrades:equity = convertToCurrency(instrument:equity [portfolio, priceSource, valuesOnSpotOverride, positionObject], portfolioFxRates, displayCurrencySymbol, filterUnit);