[Core Pricing]FDerivative:underlyingFutureMethodCandidates = select(hasMappedUnderlyingFuture, 
  nil->nil, 
  default->underlying :* "underlyingFutureMethodCandidates" [underlyingFutureObservationDate]);
[Core Pricing]FInstrument:underlyingFutureMethodCandidates = select(hasMappedUnderlyingFuture, 
  nil->nil, 
  default->underlyingFutureMethodCandidatesAndSelf);
[Core Pricing]FInstrumentAndTrades:underlyingFutureMethodCandidates = instrument:underlyingFutureMethodCandidates;
[Core Pricing]FMultiInstrumentAndTrades:underlyingFutureMethodCandidates = nil;
[Core Pricing]FPriceAggregate:underlyingFutureMethodCandidates = instrument:underlyingFutureMethodCandidates;