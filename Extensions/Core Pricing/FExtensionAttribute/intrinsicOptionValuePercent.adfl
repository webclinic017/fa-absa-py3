[Core Pricing]FInstrumentAndTrades:intrinsicOptionValuePercent = select(optionCallOrPutFactor, 
  nil->nil, 
  default->select(isFinite(openInstrumentOrInstrument:intrinsicOptionFactor [theoreticalPrice]), 
    false->nil, 
    default->(openInstrumentOrInstrument:intrinsicOptionFactor [theoreticalPrice] - 1.0) * optionCallOrPutFactor));