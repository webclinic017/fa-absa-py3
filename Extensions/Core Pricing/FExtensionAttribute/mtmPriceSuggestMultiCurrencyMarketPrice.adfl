[Core Pricing]FCurrencySwap:mtmPriceSuggestMultiCurrencyMarketPrice = legs :* "mtmPriceSuggestMultiCurrencyMarketPrice" [priceSource];
[Core Pricing]FFxSwap:mtmPriceSuggestMultiCurrencyMarketPrice = select(isGeneric, 
  true->instrument :* "marketPrice" [priceSource], 
  default->legs :* "mtmPriceSuggestMultiCurrencyMarketPrice" [priceSource]);
[Core Pricing]FInstrument:mtmPriceSuggestMultiCurrencyMarketPrice = throwError("Functionality only supported for Currency Swaps and FX Swaps");
[Core Pricing]FLeg:mtmPriceSuggestMultiCurrencyMarketPrice = instrument :* "marketPrice" [priceSource, adsPriceCurrency := currency];