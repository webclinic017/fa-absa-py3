[_securities_lending_exts]FInstrument:fixingFeeInterestAttributionValues = plZero;
[_securities_lending_exts]FInstrumentAndTrades:fixingFeeInterestAttributionValues = instrument :* "fixingFeeInterestAttributionValues" [positionObject, profitAndLossParameters, plSpotGroupingReference, createClosingLotInfos];
[_securities_lending_exts]FMultiInstrumentAndTrades:fixingFeeInterestAttributionValues = sumOf(instruments :* "fixingFeeInterestAttributionValues" [positionObject, profitAndLossParameters, plSpotGroupingReference, createClosingLotInfos]);
[_securities_lending_exts]FSecurityLoan:fixingFeeInterestAttributionValues = differential(fixingFeeInterestAttributionValueWithFixingValue, suggestedFeeInterestAttributionValue);