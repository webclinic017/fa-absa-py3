[_securities_lending_exts]FInstrument:suggestedFeeInterestAttributionValues = plZero;
[_securities_lending_exts]FInstrumentAndTrades:suggestedFeeInterestAttributionValues = instrument :* "suggestedFeeInterestAttributionValues" [positionObject, profitAndLossParameters, plSpotGroupingReference, createClosingLotInfos];
[_securities_lending_exts]FMultiInstrumentAndTrades:suggestedFeeInterestAttributionValues = sumOf(instruments :* "suggestedFeeInterestAttributionValues" [positionObject, profitAndLossParameters, plSpotGroupingReference, createClosingLotInfos]);
[_securities_lending_exts]FSecurityLoan:suggestedFeeInterestAttributionValues = differential(suggestedFeeInterestAttributionValueWithSuggestedFee, suggestedFeeInterestAttributionValue);