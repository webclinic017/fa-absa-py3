[Default]FOption:volatilityShiftedForDigitals = select(volatilityShiftTypeForDigitals, 
  "Forward"->object:volatilityUnadjusted [priceSource, doSplitAll, underlyingPriceAtForwardStartInStrikeQuotation = delta(underlyingPriceAtForwardStartInStrikeQuotation, volatilitySkewDelta)], 
  "Malz"->object:volatilityUnadjusted [priceSource, doSplitAll, malzVolatilityStrikeValue = delta(malzVolatilityStrikeValue, volatilitySkewDelta)], 
  "Other"->object:volatilityUnadjusted [priceSource, doSplitAll, volatilityStrikePrice = delta(volatilityStrikePrice, volatilitySkewDelta)]);