[Default]FQuoteController:spreadBenchmarkDiscountMarginBid = switch(spreadBenchmarkYTM, 
  nil->0.0, 
  default->switch(spreadBenchmarkYTM :* "marketDiscountMargin" [marketPrice = spreadBenchmarkYTM :* "marketPriceBid"], 
    nil->0.0, 
    default->spreadBenchmarkYTM :* "marketDiscountMargin" [marketPrice = spreadBenchmarkYTM :* "marketPriceBid"]));