[Default]FGroupingOrder:algoStartStopIcon = switch(algoCanBeStopped, 
  true->switch(algoCanBeStarted, 
    true->"Stop+Play", 
    false->"Stop"), 
  false->switch(algoCanBeStarted, 
    true->"Play", 
    false->nil));
[Default]FOrderProgram:algoStartStopIcon = switch(algoCanBeStopped, 
  true->switch(algoCanBeStarted, 
    true->"Stop+Play", 
    false->"Stop"), 
  false->switch(algoCanBeStarted, 
    true->"Play", 
    false->nil));
[Default]FOwnOrder:algoStartStopIcon = switch(algoAgentState, 
  nil->nil, 
  "Pending Execution"->"Busy", 
  "Pending Stop"->"Busy", 
  "Agent Executing"->"Stop", 
  "Agent Halted"->"Stop", 
  default->"Play");