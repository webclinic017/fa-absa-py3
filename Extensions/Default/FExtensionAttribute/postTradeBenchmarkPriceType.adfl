[Default]FGroupingOrder:postTradeBenchmarkPriceType = nil;
[Default]FOrderProgram:postTradeBenchmarkPriceType = nil;
[Default]FOwnOrder:postTradeBenchmarkPriceType = nil;
[Default]FPrePostTradeAnalysis:postTradeBenchmarkPriceType = switch(object.BenchmarkPriceType <> "-", 
  true->object.BenchmarkPriceType, 
  default->switch(object.AlgoTradingInstruction == nil, 
    true->object.BenchmarkPriceType, 
    default->switch(object.AlgoTradingInstruction.TradingStrategy, 
      "Arrival Price"->"Arrival Price", 
      "Market On Close"->"Close Price", 
      default->"VWAP")));
[Default]FSalesOrder:postTradeBenchmarkPriceType = prePostTradeAnalysis:postTradeBenchmarkPriceType;