/*
Copied from html4cssl.css to make indentation in the hierarchy and shading work.
*/

div.line-block
{
  display: block ;
  margin-top: 1em ;
  margin-bottom: 1em
  }

div.line-block div.line-block
{
  margin-top: 0 ;
  margin-bottom: 0 ;
  margin-left: 1.5em
}

