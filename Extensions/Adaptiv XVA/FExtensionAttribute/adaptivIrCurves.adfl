[Adaptiv XVA]FDictionary:adaptivIrCurves = (object.Keys) :* "adaptivIrCurves" [dict = object];
[Adaptiv XVA]FSymbol:adaptivIrCurves = select(object.StringKey, 
  "DiscountCurve"->(dict.At(object)) :* "discountCurve", 
  "ForwardCurve"->(dict.At(object)) :* "forwardCurve", 
  "RepoCurve"->(dict.At(object)) :* "repoCurve", 
  default->throwError("Key is not a curve type."));