[Adaptiv XVA]FInstrument:cvaCommodityPriceMappings = flattenAndRemoveNil([cvaDealCommodityPriceParameters]) :* "buildMarketPriceMapping";
[Adaptiv XVA]FMultiInstrumentAndTrades:cvaCommodityPriceMappings = flattenAndRemoveNil(cvaInstruments :* "cvaDealCommodityPriceParameters" [positionObject, includeAllTrades]) :* "buildMarketPriceMapping";