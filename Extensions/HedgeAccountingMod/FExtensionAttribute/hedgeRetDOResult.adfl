[HedgeAccountingMod]FDealPackageInstrumentAndTrades:hedgeRetDOResult = switch(usedInHedge, 
  default->nil, 
  true->switch(hedgeRetDOEnabled, 
    "True"->switch(getTimeSeriesDvValueFromDP(object.DealPackage, timeSeriesSpecId, hedgeRetDOResultId), 
      2.0->"Warning", 
      1.0->"Passed", 
      0.0->"Failed", 
      default->nil), 
    default->nil));
