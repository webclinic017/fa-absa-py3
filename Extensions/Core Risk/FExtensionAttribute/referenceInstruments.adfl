[Core Risk]FFuture:referenceInstruments = select(isCreditFuture, 
  true->underlying:referenceInstruments, 
  false->object);
[Core Risk]FInstrument:referenceInstruments = select(count(referenceInstrumentsFromInstrument) > 0, 
  true->createArray(referenceInstrumentsFromInstrument :* "referenceInstruments"), 
  false->object);