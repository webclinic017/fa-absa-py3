[Brokerage Risk]FInstrument:equity = equityPremium + collateralValueAdjusted;
[Brokerage Risk]FMultiInstrumentAndTrades:equity = convertToCurrency(sumOf(instruments :* "collapse(equity)" [priceSource, valuesOnSpotOverride, positionObject]), portfolioFxRates, displayCurrencySymbol, filterUnit);
[Brokerage Risk]FSingleInstrumentAndTrades:equity = convertToCurrency(instrument:equity [portfolio, priceSource, valuesOnSpotOverride, positionObject], portfolioFxRates, displayCurrencySymbol, filterUnit);