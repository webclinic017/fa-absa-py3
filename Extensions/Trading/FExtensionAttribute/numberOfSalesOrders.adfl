[Trading]FGroupingOrder:numberOfSalesOrders = collapse(numberOfMemberSalesOrders);
[Trading]FOrderProgram:numberOfSalesOrders = collapse(numberOfMemberSalesOrders);
[Trading]FOwnOrder:numberOfSalesOrders = 0;
[Trading]FSalesOrder:numberOfSalesOrders = switch(orderStatus, 
  "Active"->1, 
  "PartiallyMoved"->1, 
  "Moved"->1, 
  default->0);