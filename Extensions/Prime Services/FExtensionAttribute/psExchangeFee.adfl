[Prime Services]FInstrumentAndTrades:psExchangeFee = emptyListToZero(sumOf(trades :* "psExchangeFee"));
[Prime Services]FTrade:psExchangeFee = switch(object.AdditionalInfo.PS_ExchangeFee, 
  nil->0, 
  default->object.AdditionalInfo.PS_ExchangeFee);
[Prime Services]FTradeRow:psExchangeFee = trade:psExchangeFee;