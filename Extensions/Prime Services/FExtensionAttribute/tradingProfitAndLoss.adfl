[Prime Services]FInstrumentAndTrades:tradingProfitAndLoss = sumOf(children :* "tradingProfitAndLoss");
[Prime Services]FSingleInstrumentAndTrades:tradingProfitAndLoss = switch(isCorporateActionDay, 
  true->closingMarketValue - openingMarketValue - allInExposure + corporateActionValue.Number, 
  false->closingMarketValue - openingMarketValue - allInExposure);