[Prime Services]FInstrumentAndTrades:monthlyExecutionFee = sumOf(children :* "monthlyExecutionFee");
[Prime Services]FSingleInstrumentAndTrades:monthlyExecutionFee = switch(allFundingPortfolios, 
  true->switch(fundingPortfolioType, 
    "CFD"->-1 * py("PS_FundingCalculations", context).CalculateFee(cfdUnderlying, fundingInstruments, "ExecutionFee", isGeneralPortfolio, acm.Time.DateAddDelta(firstDayOfMonth, 0, 0, -1), profitAndLossEndDate), 
    nil->"Not implemented", 
    default->py("PS_TradeFees", context).ReturnTotalFee(tradesInPosition.AsArray, acm.Time.DateAddDelta(firstDayOfMonth, 0, 0, -1), profitAndLossEndDate)), 
  false->0);