[Securities Lending ABSA]FMoneyFlow:slProjectedFeeAccCurr = convertToCurrency(slProjectedFee, currency:immediateOrSpotFxRate [fxToCurrency = accountingCurrency, fxForwardDate = payDate], accountingCurrency);
[Securities Lending ABSA]FMoneyFlowAndTrades:slProjectedFeeAccCurr = moneyFlow:slProjectedFeeAccCurr [tradesInPosition];
[Securities Lending ABSA]FMultiMoneyFlowAndTrades:slProjectedFeeAccCurr = sumOf(moneyFlows :* "slProjectedFeeAccCurr" [tradesInPosition]);