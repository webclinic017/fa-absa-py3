[Securities Lending ABSA]FInstrument:slUtilizedQuantity = slSettledQuantity / slLendableQuantity;
[Securities Lending ABSA]FMultiInstrumentAndTrades:slUtilizedQuantity = sumOf(instruments :* "slUtilizedQuantity");
[Securities Lending ABSA]FSingleInstrumentAndTrades:slUtilizedQuantity = instrument :* "slUtilizedQuantity";