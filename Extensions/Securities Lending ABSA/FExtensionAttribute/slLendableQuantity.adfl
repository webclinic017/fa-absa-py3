[Securities Lending ABSA]FInstrument:slLendableQuantity = slLendableQuantity = slHoldingQuantity / 2;
[Securities Lending ABSA]FMultiInstrumentAndTrades:slLendableQuantity = sumOf(instruments :* "slLendableQuantity");
[Securities Lending ABSA]FSingleInstrumentAndTrades:slLendableQuantity = instrument :* "slLendableQuantity";