[Securities Lending ABSA]FMoneyFlow:slFeeVat = slProjectedFeeAccCurr * (slFeeVatRate - 1) / slFeeVatRate;
[Securities Lending ABSA]FMoneyFlowAndTrades:slFeeVat = moneyFlow:slFeeVat [tradesInPosition];
[Securities Lending ABSA]FMultiMoneyFlowAndTrades:slFeeVat = sumOf(moneyFlows :* "slFeeVat" [tradesInPosition]);