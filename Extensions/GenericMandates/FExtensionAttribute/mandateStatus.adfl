[GenericMandates]FBusinessProcess:mandateStatus = switch(object.CurrentStep.State.Name, 
  "Active"->"COMPLETED", 
  "Rejected"->"TERMINATED", 
  default->"IN PROGRESS");
[GenericMandates]FLimit:mandateStatus = switch(limitCurrentState, 
  "Active"->"ENABLED", 
  "Warning"->"ENABLED", 
  "Breached"->"ENABLED", 
  default->"DISABLED");