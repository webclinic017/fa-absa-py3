[PreAndPostTradeAnalysis]FGroupingOrder:preTradePeriodAverageTradeSize = nil;
[PreAndPostTradeAnalysis]FOrderProgram:preTradePeriodAverageTradeSize = nil;
[PreAndPostTradeAnalysis]FOwnOrder:preTradePeriodAverageTradeSize = nil;
[PreAndPostTradeAnalysis]FPrePostTradeAnalysis:preTradePeriodAverageTradeSize = switch(preTradeAnalysisHasPeriod, 
  false->0, 
  default->switch(preTradePeriodAverageNumberOfTrades, 
