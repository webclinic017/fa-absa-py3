[PreAndPostTradeAnalysis]FPrePostTradeAnalysis:marketTradingHours = switch(homeTrading, 
  nil->0, 
  default->homeTrading :* "marketTradingHours");
[PreAndPostTradeAnalysis]FSalesOrder:marketTradingHours = prePostTradeAnalysis:marketTradingHours;
[PreAndPostTradeAnalysis]FTradingInterface:marketTradingHours = 24.0 * (marketClosingTime - marketOpeningTime);